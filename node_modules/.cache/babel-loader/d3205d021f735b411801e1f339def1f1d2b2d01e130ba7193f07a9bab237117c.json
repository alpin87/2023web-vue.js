{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nimport cheerio from 'cheerio';\nexport default {\n  data() {\n    return {\n      stocks: []\n    };\n  },\n  methods: {\n    async fetchStockData() {\n      try {\n        const response = await axios.get('https://kr.investing.com/stock-screener/?sp=country::11|sector::a|industry::a|equityType::a%3Ceq_market_cap;1');\n        const $ = cheerio.load(response.data);\n        const stocks = [];\n        $('#resultsTable tr[data-pair-id]').each((index, element) => {\n          const symbol = $(element).find('.symbol a').text().trim();\n          const name = $(element).find('.name a').text().trim();\n          const last = $(element).find('.last-price span').text().trim();\n          const high = $(element).find('.high-price span').text().trim();\n          const low = $(element).find('.low-price span').text().trim();\n          const change = $(element).find('.change-price').text().trim();\n          const changePercentage = $(element).find('.change-percent').text().trim();\n          const volume = $(element).find('.volume').text().trim();\n          const marketCap = $(element).find('.market-cap').text().trim();\n          stocks.push({\n            symbol,\n            name,\n            last,\n            high,\n            low,\n            change,\n            changePercentage,\n            volume,\n            marketCap\n          });\n        });\n        this.stocks = stocks;\n      } catch (error) {\n        console.error('Error:', error);\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","cheerio","data","stocks","methods","fetchStockData","response","get","$","load","each","index","element","symbol","find","text","trim","name","last","high","low","change","changePercentage","volume","marketCap","push","error","console"],"sources":["C:\\Users\\qortm\\Desktop\\vueweb\\vue_web\\src\\views\\SearchView.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <button @click=\"fetchStockData\">Fetch Stock Data</button>\r\n\r\n    <table>\r\n      <thead>\r\n        <tr>\r\n          <th>Symbol</th>\r\n          <th>Name</th>\r\n          <th>Last</th>\r\n          <th>High</th>\r\n          <th>Low</th>\r\n          <th>Change</th>\r\n          <th>Change %</th>\r\n          <th>Volume</th>\r\n          <th>Market Cap</th>\r\n        </tr>\r\n      </thead>\r\n      <tbody>\r\n        <tr v-for=\"stock in stocks\" :key=\"stock.symbol\">\r\n          <td>{{ stock.symbol }}</td>\r\n          <td>{{ stock.name }}</td>\r\n          <td>{{ stock.last }}</td>\r\n          <td>{{ stock.high }}</td>\r\n          <td>{{ stock.low }}</td>\r\n          <td>{{ stock.change }}</td>\r\n          <td>{{ stock.changePercentage }}</td>\r\n          <td>{{ stock.volume }}</td>\r\n          <td>{{ stock.marketCap }}</td>\r\n        </tr>\r\n      </tbody>\r\n    </table>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\nimport cheerio from 'cheerio';\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      stocks: []\r\n    };\r\n  },\r\n  methods: {\r\n    async fetchStockData() {\r\n      try {\r\n        const response = await axios.get('https://kr.investing.com/stock-screener/?sp=country::11|sector::a|industry::a|equityType::a%3Ceq_market_cap;1');\r\n        const $ = cheerio.load(response.data);\r\n        const stocks = [];\r\n\r\n        $('#resultsTable tr[data-pair-id]').each((index, element) => {\r\n          const symbol = $(element).find('.symbol a').text().trim();\r\n          const name = $(element).find('.name a').text().trim();\r\n          const last = $(element).find('.last-price span').text().trim();\r\n          const high = $(element).find('.high-price span').text().trim();\r\n          const low = $(element).find('.low-price span').text().trim();\r\n          const change = $(element).find('.change-price').text().trim();\r\n          const changePercentage = $(element).find('.change-percent').text().trim();\r\n          const volume = $(element).find('.volume').text().trim();\r\n          const marketCap = $(element).find('.market-cap').text().trim();\r\n\r\n          stocks.push({\r\n            symbol,\r\n            name,\r\n            last,\r\n            high,\r\n            low,\r\n            change,\r\n            changePercentage,\r\n            volume,\r\n            marketCap\r\n          });\r\n        });\r\n\r\n        this.stocks = stocks;\r\n      } catch (error) {\r\n        console.error('Error:', error);\r\n      }\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\ntable {\r\n  width: 100%;\r\n}\r\n\r\nth,\r\ntd {\r\n  padding: 8px;\r\n  text-align: left;\r\n}\r\n\r\nth {\r\n  background-color: #f2f2f2;\r\n}\r\n</style>\r\n"],"mappings":";AAoCA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,OAAM,MAAO,SAAS;AAE7B,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,MAAM,EAAE;IACV,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,cAAcA,CAAA,EAAG;MACrB,IAAI;QACF,MAAMC,QAAO,GAAI,MAAMN,KAAK,CAACO,GAAG,CAAC,+GAA+G,CAAC;QACjJ,MAAMC,CAAA,GAAIP,OAAO,CAACQ,IAAI,CAACH,QAAQ,CAACJ,IAAI,CAAC;QACrC,MAAMC,MAAK,GAAI,EAAE;QAEjBK,CAAC,CAAC,gCAAgC,CAAC,CAACE,IAAI,CAAC,CAACC,KAAK,EAAEC,OAAO,KAAK;UAC3D,MAAMC,MAAK,GAAIL,CAAC,CAACI,OAAO,CAAC,CAACE,IAAI,CAAC,WAAW,CAAC,CAACC,IAAI,CAAC,CAAC,CAACC,IAAI,CAAC,CAAC;UACzD,MAAMC,IAAG,GAAIT,CAAC,CAACI,OAAO,CAAC,CAACE,IAAI,CAAC,SAAS,CAAC,CAACC,IAAI,CAAC,CAAC,CAACC,IAAI,CAAC,CAAC;UACrD,MAAME,IAAG,GAAIV,CAAC,CAACI,OAAO,CAAC,CAACE,IAAI,CAAC,kBAAkB,CAAC,CAACC,IAAI,CAAC,CAAC,CAACC,IAAI,CAAC,CAAC;UAC9D,MAAMG,IAAG,GAAIX,CAAC,CAACI,OAAO,CAAC,CAACE,IAAI,CAAC,kBAAkB,CAAC,CAACC,IAAI,CAAC,CAAC,CAACC,IAAI,CAAC,CAAC;UAC9D,MAAMI,GAAE,GAAIZ,CAAC,CAACI,OAAO,CAAC,CAACE,IAAI,CAAC,iBAAiB,CAAC,CAACC,IAAI,CAAC,CAAC,CAACC,IAAI,CAAC,CAAC;UAC5D,MAAMK,MAAK,GAAIb,CAAC,CAACI,OAAO,CAAC,CAACE,IAAI,CAAC,eAAe,CAAC,CAACC,IAAI,CAAC,CAAC,CAACC,IAAI,CAAC,CAAC;UAC7D,MAAMM,gBAAe,GAAId,CAAC,CAACI,OAAO,CAAC,CAACE,IAAI,CAAC,iBAAiB,CAAC,CAACC,IAAI,CAAC,CAAC,CAACC,IAAI,CAAC,CAAC;