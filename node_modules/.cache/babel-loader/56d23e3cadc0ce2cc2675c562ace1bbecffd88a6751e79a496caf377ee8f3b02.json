 }\r\n      } catch (error) {\r\n        console.error('Error: ', error)\r\n      }\r\n    }\r\n  },\r\n  mounted () {\r\n    this.fetchExchangeData()\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n#exchange-table {\r\n  width: 100%;\r\n  max-width: 1800px;\r\n  margin-left : 200px;\r\n  border-collapse: collapse;\r\n  border: 1px solid #ddd;\r\n  box-shadow: 0px 0px 20px 0px rgba(0,0,0,0.15);\r\n}\r\n\r\n#exchange-table th, #exchange-table td {\r\n  padding: 15px;\r\n  border-bottom: 1px solid #ddd;\r\n}\r\n\r\n#exchange-table th {\r\n  background-color: #4caf50;\r\n  color: white;\r\n  text-transform: uppercase;\r\n}\r\n\r\n#exchange-table tr:nth-child(even) {\r\n  background-color: #f8f8f8;\r\n}\r\n</style>\r\n"],"mappings":"AAsCA,OAAOA,KAAI,MAAO,OAAM;AAExB,eAAe;EACbC,IAAGA,CAAA,EAAK;IACN,OAAO;MACLC,aAAa,EAAE,EAAE;MACjBC,OAAO,EAAE,kCAAkC;MAC3CC,UAAU,EAAE,EAAE;MACdH,IAAI,EAAE,MAAK,CAAE;IACf;EACF,CAAC;;EACDI,OAAO,EAAE;IACP,MAAMC,iBAAgBA,CAAA,EAAK;MACzB,MAAMC,GAAE,GAAI,yHAAwH;MAEpI,IAAI;QACF,MAAM;UAAEN;QAAK,IAAI,MAAMD,KAAK,CAACQ,GAAG,CAACD,GAAG;QACpC,IAAIN,IAAI,CAACQ,MAAK,KAAM,CAAC,EAAE;UACrB,IAAI,CAACP,aAAY,GAAID,IAAI,CAACC,aAAa,CAACQ,GAAG,CAACC,IAAG,KAAM;YACnDC,QAAQ,EAAED,IAAI,CAACE,QAAQ;YACvBC,MAAM,EAAEH,IAAI,CAACI,MAAM;YACnBC,GAAG,EAAEL,IAAI,CAACM,GAAG;YACbC,GAAG,EAAEP,IAAI,CAACQ,GAAG;YACbC,UAAU,EAAET,IAAI,CAACU,UAAU;YAC3BC,IAAI,EAAEX,IAAI,CAACY,IAAI;YACfC,SAAS,EAAEb,IAAI,CAACc,SAAS;YACzBC,aAAa,EAAEf,IAAI,CAACgB,aAAa;YACjCC,eAAe,EAAEjB,IAAI,CAACkB,eAAe;YACrCC,SAAS,EAAEnB,IAAI,CAACoB;UAClB,CAAC,CAAC;QACJ,OAAO;UACLC,OAAO,CAACC,KAAK,CAAC,SAAS,EAAEhC,IAAI,CAACQ,MAAM;QACtC;MACF,EAAE,OAAOwB,KAAK,EAAE;QACdD,OAAO,CAACC,KAAK,CAAC,SAAS,EAAEA,KAAK;MAChC;IACF;EACF,CAAC;EACDC,OAAMA,CAAA,EAAK;IACT,IAAI,CAAC5B,iBAAiB,CAAC;EACzB;AACF"},"metadata":{},"sourceType":"module","externalDependencies":[]}